{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ts-monorepo-template",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue"
  },
  "workspaces": {
    "packages": [
      "package/*"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "eslint": "^8.15.0",
    "prettier": "^2.6.2",
    "turbo": "^1.2.9",
    "typescript": "next",
    "vite": "^2.9.9"
  }
}
