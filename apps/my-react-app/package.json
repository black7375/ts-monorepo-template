{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "yarn run -T rimraf dist coverage .turbo tsconfig.tsbuildinfo node_modules/.vite",
    "start": "yarn run -T vite",
    "preview": "yarn run -T vite preview",
    "build": "yarn run -T vite build",
    "build:release": "yarn run -T vite build",
    "build:watch": "yarn run build --watch",
    "lint": "yarn run -T eslint . --report-unused-disable-directives --max-warnings 0",
    "fix": "yarn run lint --fix",
    "check": "yarn run -T tsc --noEmit",
    "storybook": "yarn run -T storybook dev -p 6006",
    "build-storybook": "yarn run -T storybook build"
  },
  "prettier": "prettier-config-custom",
  "devDependencies": {
    "eslint-config-react": "workspace:^",
    "eslint-plugin-storybook": "^0.6.12",
    "prettier-config-custom": "workspace:^",
    "tsconfig-custom": "workspace:^",
    "vite-config-custom": "workspace:^"
  }
}
